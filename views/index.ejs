 function   hiiden() {
  
  document.getElementById("fixedtable").style.display = "";

  
  var elements = document.getElementsByClassName("noprinted");
  for (var i = 0; i < elements.length; i++) {
    var element = elements[i];
    if (element.style.display === "none") {
      element.style.display = "block"; // Show the element if it's currently hidden
    } else {
      element.style.display = "none"; // Hide the element if it's currently visible
    }
  }
};




